#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

cp -r $INDIR/I8/test2.tar ./

{ timeout -k 9 ${TIMEOUT}s $BIN -xf test2.tar --to-command='mkdir $TAR_REALNAME'; } &>$OUTDIR/o631

echo "$?" >>$OUTDIR/o631
test -d test
echo "$?" >>$OUTDIR/o631
test -d test1
echo "$?" >>$OUTDIR/o631
test -d test2
echo "$?" >>$OUTDIR/o631
test -d test3
echo "$?" >>$OUTDIR/o631
test -d test4
echo "$?" >>$OUTDIR/o631
test -d filenamenotcontainingtest
echo "$?" >>$OUTDIR/o631

#I8