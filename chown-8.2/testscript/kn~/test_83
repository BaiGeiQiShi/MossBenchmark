#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

uname=$(whoami)

cp $INDIR/I7/tmpfile ./

{ timeout -k 9 ${TIMEOUT}s $BIN --from=:${uname} :sudo tmpfile; } &>$OUTDIR/o83

echo "$?" >>$OUTDIR/o83
own0=`ls -al tmpfile | tr -s ' ' | cut -d ' ' -f 3`
own1=`ls -al tmpfile | tr -s ' ' | cut -d ' ' -f 4`
echo "${own0} ${own1}" >>$OUTDIR/o83

#parentI77