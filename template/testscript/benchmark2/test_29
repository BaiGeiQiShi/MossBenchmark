#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

uname=$(whoami)

cp $INDIR/I3/1.txt ./

{ timeout -k 9 ${TIMEOUT}s $BIN --from=:${uname} :sudo 1.txt; } &>$OUTDIR/o29

echo "$?" >>$OUTDIR/o29
own0=`ls -al 1.txt | tr -s ' ' | cut -d ' ' -f 3`
own1=`ls -al 1.txt | tr -s ' ' | cut -d ' ' -f 4`
echo "${own0} ${own1}" >>$OUTDIR/o29

#parentI34