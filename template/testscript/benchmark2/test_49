#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

uname=$(whoami)
uid=`id -u ${uname}`

cp -r $INDIR/I5/Dir1 ./

{ timeout -k 9 ${TIMEOUT}s $BIN -R ${uname}:sudo Dir1; } &>$OUTDIR/o49

echo "$?" >>$OUTDIR/o49
own0=`ls -al Dir1 | tr -s ' ' | cut -d ' ' -f 3`
own1=`ls -al Dir1 | tr -s ' ' | cut -d ' ' -f 4`
echo "${own0} ${own1}" >>$OUTDIR/o49

#parentI513