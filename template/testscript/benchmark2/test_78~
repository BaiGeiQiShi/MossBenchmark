#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

uname=$(whoami)

cp $INDIR/I7/tmpfile ./

{ timeout -k 9 ${TIMEOUT}s $BIN ${uname}:sudo tmpfile; } &>$OUTDIR/o78

echo "$?" >>$OUTDIR/o78
own0=`ls -al tmpfile | tr -s ' ' | cut -d ' ' -f 3`
own1=`ls -al tmpfile | tr -s ' ' | cut -d ' ' -f 4`
echo "${own0} ${own1}" >>$OUTDIR/o78

#parentI72